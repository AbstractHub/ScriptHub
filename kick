local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local player = Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "PrivateServerWarningGui"
screenGui.Parent = player:WaitForChild("PlayerGui")
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.4, 0, 0.2, 0)
frame.Position = UDim2.new(0.3, 0, 0.4, 0)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true
frame.BackgroundTransparency = 1
frame.Parent = screenGui
local uiStroke = Instance.new("UIStroke")
uiStroke.Thickness = 2
uiStroke.Color = Color3.fromRGB(255, 0, 0)
uiStroke.Parent = frame
local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(0, 10)
uiCorner.Parent = frame
local uiShadow = Instance.new("ImageLabel")
uiShadow.Size = UDim2.new(1, 20, 1, 20)
uiShadow.Position = UDim2.new(0, -10, 0, -10)
uiShadow.BackgroundTransparency = 1
uiShadow.Image = "rbxassetid://1316045217"
uiShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
uiShadow.ImageTransparency = 1
uiShadow.ZIndex = -1
uiShadow.Parent = frame
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(1, -20, 0.7, -10)
textLabel.Position = UDim2.new(0, 10, 0, 5)
textLabel.BackgroundTransparency = 1
textLabel.Text = "If you are using a private server, the script might not function properly. Using a public server is highly suggested."
textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
textLabel.TextTransparency = 1
textLabel.TextScaled = true
textLabel.Font = Enum.Font.GothamSemibold
textLabel.Parent = frame
local okButton = Instance.new("TextButton")
okButton.Size = UDim2.new(0.35, 0, 0.15, 0)  -- Smaller size
okButton.Position = UDim2.new(0.1, 0, 0.75, 0)  -- Adjusted position (moved slightly left)
okButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
okButton.Text = "Yeah Whatever"
okButton.TextColor3 = Color3.fromRGB(255, 255, 255)
okButton.TextTransparency = 1
okButton.TextScaled = true
okButton.Font = Enum.Font.GothamBold
okButton.Parent = frame
local buttonCorner = Instance.new("UICorner")
buttonCorner.CornerRadius = UDim.new(0, 8)
buttonCorner.Parent = okButton
local teleportButton = Instance.new("TextButton")
teleportButton.Size = UDim2.new(0.35, 0, 0.15, 0)  -- Smaller size
teleportButton.Position = UDim2.new(0.55, 0, 0.75, 0)  -- Adjusted position (moved slightly right)
teleportButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)  -- Green color
teleportButton.Text = "Take Me There"
teleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
teleportButton.TextTransparency = 1
teleportButton.TextScaled = true
teleportButton.Font = Enum.Font.GothamBold
teleportButton.Parent = frame
local teleportButtonCorner = Instance.new("UICorner")
teleportButtonCorner.CornerRadius = UDim.new(0, 8)
teleportButtonCorner.Parent = teleportButton

-- Hover effect
local function onButtonHoverIn(button)
    button:TweenSize(UDim2.new(button.Size.X.Scale * 1.1, 0, button.Size.Y.Scale * 1.1, 0), "Out", "Quad", 0.1, true)
end

local function onButtonHoverOut(button)
    button:TweenSize(UDim2.new(button.Size.X.Scale / 1.1, 0, button.Size.Y.Scale / 1.1, 0), "Out", "Quad", 0.1, true)
end

okButton.MouseEnter:Connect(function()
    onButtonHoverIn(okButton)
end)

okButton.MouseLeave:Connect(function()
    onButtonHoverOut(okButton)
end)

teleportButton.MouseEnter:Connect(function()
    onButtonHoverIn(teleportButton)
end)

teleportButton.MouseLeave:Connect(function()
    onButtonHoverOut(teleportButton)
end)

for i = 1, 0, -0.1 do
    frame.BackgroundTransparency = i
    textLabel.TextTransparency = i
    okButton.BackgroundTransparency = i
    okButton.TextTransparency = i
    teleportButton.BackgroundTransparency = i
    teleportButton.TextTransparency = i
    uiShadow.ImageTransparency = 0.7 + i * 0.3
    wait(0.05)
end

local function closeGui()
    for i = 0, 1, 0.1 do
        frame.BackgroundTransparency = i
        textLabel.TextTransparency = i
        okButton.BackgroundTransparency = i
        okButton.TextTransparency = i
        teleportButton.BackgroundTransparency = i
        teleportButton.TextTransparency = i
        uiShadow.ImageTransparency = 0.7 + i * 0.3
        wait(0.05)
    end
    screenGui:Destroy()
end

okButton.MouseButton1Click:Connect(function()
    closeGui()
end)

teleportButton.MouseButton1Click:Connect(function()
    closeGui()
    TeleportService:Teleport(16732694052, player)
end)
